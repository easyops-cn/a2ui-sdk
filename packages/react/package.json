{
  "name": "@a2ui-sdk/react",
  "version": "0.3.0",
  "description": "A2UI SDK for React",
  "homepage": "https://a2ui-sdk.js.org/",
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/a2ui-sdk.git",
    "directory": "packages/react"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./0.8": {
      "types": "./dist/0.8/index.d.ts",
      "default": "./dist/0.8/index.js"
    },
    "./0.8/standard-catalog": {
      "types": "./dist/0.8/standard-catalog/index.d.ts",
      "default": "./dist/0.8/standard-catalog/index.js"
    },
    "./0.9": {
      "types": "./dist/0.9/index.d.ts",
      "default": "./dist/0.9/index.js"
    },
    "./0.9/standard-catalog": {
      "types": "./dist/0.9/standard-catalog/index.d.ts",
      "default": "./dist/0.9/standard-catalog/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b tsconfig.build.json && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "jsdom": "^27.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@a2ui-sdk/types": "0.3.0",
    "@a2ui-sdk/utils": "0.3.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0"
  }
}
