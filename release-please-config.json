{
  "packages": {
    ".": {
      "component": "root"
    },
    "packages/types": {
      "component": "types"
    },
    "packages/utils": {
      "component": "utils"
    },
    "packages/react": {
      "component": "react"
    }
  },
  "bump-minor-pre-major": true,
  "include-component-in-tag": true,
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "group-name": "a2ui-sdk",
      "components": ["root", "types", "utils", "react"]
    }
  ]
}
