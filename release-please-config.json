{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    "packages/types": {
      "component": "types"
    },
    "packages/utils": {
      "component": "utils"
    },
    "packages/react": {
      "component": "react"
    }
  },
  "release-type": "node",
  "bump-minor-pre-major": true,
  "include-component-in-tag": true,
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "A2UI SDK",
      "components": ["types", "utils", "react"]
    }
  ]
}
